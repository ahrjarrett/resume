{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "composite": true,
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017"],
    "importsNotUsedAsValues": "remove",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  }
}
