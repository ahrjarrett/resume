{
  "compilerOptions": {
    "composite": true,
    "module": "commonjs",
    "target": "es2017",
    "lib": ["esnext", "dom"],
    "importsNotUsedAsValues": "remove",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
